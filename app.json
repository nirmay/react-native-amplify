{
  "expo": {
    "name": "PoC - ReactNative + Amplify",
    "slug": "nirmay-reactnative-amplify",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
